<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:viewModel="clr-namespace:AnimationMaker.ViewModel"
                    xmlns:resources="clr-namespace:AnimationMaker.Resources">
	<DataTemplate DataType="{x:Type viewModel:IPointViewModel}" x:Key="PointDataTemplate">
		<Ellipse Canvas.Left="{Binding X}" Canvas.Right="{Binding Y}" Width="2" Height="2" />
	</DataTemplate>
	<DataTemplate DataType="{x:Type viewModel:IEdgeViewModel}" x:Key="EdgeDataTemplate">
		<Line X1="{Binding Start.X}" X2="{Binding End.X}" Y1="{Binding Start.Y}" Y2="{Binding End.Y}" StrokeThickness="2" />
	</DataTemplate>
	<resources:FiguresTemplateSelector x:Key="FiguresTemplateSelector"
	                                   PointTemplate="{StaticResource PointDataTemplate}"
	                                   EdgeTemplate="{StaticResource EdgeDataTemplate}" />
</ResourceDictionary>